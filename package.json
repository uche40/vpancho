{
  "name": "vuero",
  "version": "1.0.3",
  "private": "true",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./scripts/skip.js || husky install",
    "dev": "vite",
    "dev:force": "vite --force",
    "preview": "vite preview",
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:vite": "vite build",
    "lint:fix": "run-s  lint:doc:eslint lint:doc:prettier lint:eslint:fix lint:stylelint:fix lint:prettier:fix",
    "lint:prettier": "prettier --check \"./src/**/*.(js|ts|vue|css|scss|graphql|gql)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(js|ts|vue|css|scss|graphql|gql)\"",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:doc:fix": "run-s lint:doc:*",
    "lint:doc:eslint": "eslint --fix --ext .md ./documentation",
    "lint:doc:prettier": "prettier --write \"./documentation/**/*.(md)\"",
    "test:check": "vuedx-typecheck .",
    "test:lint": "run-s lint:eslint lint:stylelint lint:prettier",
    "test:e2e": "cypress run",
    "precommit": "run-s precommit:*",
    "precommit:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "26.0.0",
    "@ckeditor/ckeditor5-vue": "2.0.1",
    "@iconify/iconify": "1.0.7",
    "@popperjs/core": "2.9.0",
    "@vueform/multiselect": "1.3.2",
    "@vueform/slider": "1.0.3",
    "@vueuse/core": "4.3.3",
    "@vueuse/head": "0.5.1",
    "apexcharts": "3.25.0",
    "axios": "0.21.1",
    "billboard.js": "2.2.5",
    "bulma": "0.9.2",
    "dayjs": "1.10.4",
    "dragula": "3.7.3",
    "dropzone": "5.7.6",
    "filepond": "4.25.1",
    "filepond-plugin-file-validate-size": "2.2.2",
    "filepond-plugin-file-validate-type": "1.2.5",
    "filepond-plugin-image-crop": "2.0.4",
    "filepond-plugin-image-edit": "1.6.2",
    "filepond-plugin-image-exif-orientation": "1.0.9",
    "filepond-plugin-image-preview": "4.6.5",
    "filepond-plugin-image-resize": "2.0.7",
    "filepond-plugin-image-transform": "3.7.5",
    "imask": "6.0.7",
    "markdown-it-emoji": "2.0.0",
    "notyf": "3.9.0",
    "nprogress": "0.2.0",
    "photoswipe": "4.1.3",
    "simple-datatables": "3.0.2",
    "simplebar": "6.0.0-beta.9",
    "simplebar-vue": "1.6.0",
    "tiny-slider": "2.9.3",
    "tippy.js": "6.3.1",
    "v-calendar": "3.0.0-alpha.3",
    "vue": "3.0.7",
    "vue-i18n": "9.0.0-rc.9",
    "vue-router": "4.0.4",
    "vue-scrollto": "2.20.0",
    "vue-tippy": "6.0.0-alpha.25",
    "vue3-apexcharts": "1.2.1"
  },
  "devDependencies": {
    "@iconify/json": "1.1.312",
    "@intlify/vite-plugin-vue-i18n": "2.0.1",
    "@types/dragula": "3.7.0",
    "@types/markdown-it": "12.0.1",
    "@types/node": "14.14.31",
    "@types/nprogress": "0.2.0",
    "@types/prismjs": "1.16.3",
    "@types/simplebar": "5.1.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "@vitejs/plugin-vue": "1.1.5",
    "@vue/compiler-sfc": "3.0.7",
    "@vue/server-renderer": "3.0.7",
    "@vuedx/typecheck": "0.6.3",
    "@vuedx/typescript-plugin-vue": "0.6.3",
    "cross-env": "7.0.3",
    "cypress": "6.6.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-md": "1.0.19",
    "eslint-plugin-prettier-vue": "3.0.0-alpha.2",
    "eslint-plugin-vue": "7.7.0",
    "gray-matter": "4.0.2",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "markdown-it": "12.0.4",
    "markdown-it-anchor": "7.0.2",
    "npm-run-all": "4.1.5",
    "plyr": "3.6.4",
    "prettier": "2.2.1",
    "prismjs": "1.23.0",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "sass": "1.32.8",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.19.0",
    "typescript": "4.2.3",
    "vite": "2.0.5",
    "vite-imagetools": "2.3.3",
    "vite-plugin-components": "0.8.0",
    "vite-plugin-fonts": "0.1.5",
    "vite-plugin-pages": "0.5.1",
    "vite-plugin-purge-icons": "0.7.0",
    "vite-plugin-pwa": "0.5.6",
    "vite-plugin-radar": "^0.0.1",
    "yorkie": "2.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint",
      "prettier --write"
    ],
    "*.md": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
